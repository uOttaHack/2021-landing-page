{
  "hosting": {
    "target": "landing-page",
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/apply",
        "destination": "/apply.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
